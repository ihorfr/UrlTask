@model IEnumerable<WebApplication1.Models.UrlInfo>

@{
    ViewBag.Title = "UrlStatistics";
}

<h3>Url Statistics</h3>

<table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.URL)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastRequestDate)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastTitle)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LastStatus)
            </th>
            <th></th>
        </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.URL)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastRequestDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastTitle)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastStatus)
            </td>
            <td>
                @Html.ActionLink("Graf", "Graf", new { url = item.URL })
            </td>
        </tr>
    }
        </table>
    



